{
  "name": "resume-job-portal-backend",
  "version": "2.0.0",
  "description": "Complete Resume Builder and Job Portal Backend API with Google OAuth, Templates, and Subscriptions",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "seed": "node src/scripts/seed.js",
    "seed:themes": "node scripts/seed-themes-from-frontend.js",
    "seed:section-layouts": "node scripts/seed-section-layouts-from-frontend.js",
    "seed:all-frontend": "npm run seed:themes && npm run seed:section-layouts",
    "test": "jest --coverage",
    "lint": "eslint .",
    "format": "prettier --write \"**/*.js\"",
    "init-plans": "node src/scripts/initPlans.js"
  },
  "keywords": [
    "resume",
    "job-portal",
    "nodejs",
    "express",
    "mongodb",
    "oauth",
    "subscription"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cloudinary": "^2.8.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-session": "^1.17.3",
    "express-validator": "^7.0.1",
    "google-auth-library": "^9.14.2",
    "handlebars": "^4.7.8",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.29.4",
    "mongoose": "^8.0.3",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^7.0.12",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "pdfkit": "^0.14.0",
    "razorpay": "^2.9.2",
    "stripe": "^14.7.0"
  },
  "devDependencies": {
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.1"
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  }
}